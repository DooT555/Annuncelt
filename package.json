{
  "name": "announceit",
  "version": "2.0.0",
  "description": "",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node . | pino-pretty",
    "start:build": "pnpm run build && pnpm run start",
    "start:dev": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"node .\"",
    "lint": "eslint .",
    "fix": "eslint . --fix"
  },
  "keywords": [],
  "author": "Vicente015 <me@vicente015.dev>",
  "license": "MIT",
  "engines": {
    "node": ">=18",
    "pnpm": ">=7",
    "typescript": ">=4.8.4",
    "tsc": ">=4.8.4"
  },
  "dependencies": {
    "@cospired/i18n-iso-languages": "~4.0.0",
    "@discordjs/builders": "~1.4.0",
    "@discordjs/collection": "~1.3.0",
    "@sapphire/discord-utilities": "~2.12.0",
    "@sapphire/framework": "~3.2.0",
    "@sapphire/plugin-i18next": "~4.0.3",
    "@sapphire/plugin-subcommands": "~3.2.4",
    "colord": "~2.9.3",
    "discord-api-types": "~0.37.26",
    "discord.js": "~13.12.0",
    "dotenv": "~10.0.0",
    "iso-639-1": "^2.1.15",
    "mongoose": "~6.7.5",
    "ow": "0.28.2",
    "pagination.djs": "~3.6.5",
    "zod": "~3.20.2"
  },
  "devDependencies": {
    "@tsconfig/node18": "^1.0.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": ">=5.0.0 <6.0.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-n": "^15.0.1",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-sort": "^2.4.0",
    "eslint-plugin-unicorn": "^44.0.2",
    "tsc-watch": "^6.0.0",
    "typescript": "*"
  },
  "eslintConfig": {
    "extends": [
      "standard-with-typescript",
      "plugin:sort/recommended",
      "plugin:unicorn/recommended"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "@typescript-eslint/no-non-null-assertion": "warn",
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/strict-boolean-expressions": "off",
      "@typescript-eslint/restrict-template-expressions": "off",
      "@typescript-eslint/no-base-to-string": "off",
      "unicorn/consistent-function-scoping": "off",
      "unicorn/no-await-expression-member": "off",
      "unicorn/prefer-top-level-await": "off",
      "unicorn/no-array-reduce": "off",
      "unicorn/filename-case": "off",
      "unicorn/prefer-module": "off"
    },
    "overrides": [
      {
        "files": [
          "src/schemas/*"
        ],
        "rules": {
          "sort/object-properties": "off"
        }
      }
    ],
    "ignorePatterns": [
      "dist",
      "src/**/*.js"
    ]
  }
}
